function inpos = moveintoUnitcell(frac_pos, frac_pos2move)
% move cart_pos into unitcell based on its CMS frac_pos
% frac_pos should be 1x3 matrix.
% cart_pos should be mx3 matrix.
if nargin < 2
    frac_pos2move = frac_pos;
end
in_frac_pos = rem((frac_pos), 1);
ind = find(in_frac_pos<0);
in_frac_pos(ind) = in_frac_pos(ind) + 1;
dv = frac_pos - in_frac_pos;
inpos = cart_pos - dv*cellinfo.latticevectors;